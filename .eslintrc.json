{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "error",
        "class-methods-use-this":"off",
        "no-throw-literal": "off",
        "no-restricted-syntax":"off",
        "no-await-in-loop": "off",
        "consistent-return": "off",
        "no-underscore-dangle":"off",
        "camelcase":"off"
    }
}
